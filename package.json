{
  "name": "koa2-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node bin/www",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
    "debug": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --inspect=9229 bin/www",
    "prd": "cross-env NODE_ENV=production pm2 start bin/www",
    "lint": "eslint --ext .js ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seq:sync": "cross-env NODE_ENV=development node ./db/seq/sync",
    "test:mocha": "cross-env NODE_ENV=test mocha"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "cross-env": "^7.0.3",
    "debug": "^4.1.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^2.1.1",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-redis": "^4.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "mysql2": "^2.2.5",
    "pug": "^2.0.3",
    "redis": "^3.0.2",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "mocha": "^8.3.0",
    "nodemon": "^1.19.1",
    "pre-commit": "^1.2.2",
    "supertest": "^6.1.3"
  },
  "pre-commit": [
    "lint"
  ]
}
